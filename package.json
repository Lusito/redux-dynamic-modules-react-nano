  "devDependencies": {
    "@lusito/eslint-config-react": "^1.3.0",
    "@lusito/prettier-config": "^1.2.0",
    "@lusito/stylelint-config": "^1.2.0",
    "@types/react": "^16.9.49",
    "react": "^16.13.1",
    "react-redux-nano": "^1.1.0",
    "redux": "^4.0.5",
    "redux-dynamic-modules-core": "^5.2.3",
    "sort-package-json": "^1.44.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-redux-nano": "^1.0.0",
    "redux": "^4.0.0",
    "redux-dynamic-modules-core": "^5.0.0"
  }
